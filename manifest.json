{
  "manifest_version": 2,

  "name": "RandomNews Plugin",
  "description": "This extension will give you a random news",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon16.png",
   "default_popup": "popup.html"
  },
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "permissions": [
   "activeTab"
   ]
}